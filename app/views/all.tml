{{ define "content" }}
<section class="inbox">
  <div class="title">
    <label>{{.Total}} Inbox</label>
  </div>
  <div class="items">
   <ul>
    {{range $key, $item := .Items}}
      <li>
       <a href='/inbox/{{$key}}'>
          <label class="item_name">{{$key}}</label>
          <label class="item_size">[Has {{$item}} Items]</label>
        </a>
      </li>
    {{end}}
    </ul>
  </div>
</section>
{{ end }}
