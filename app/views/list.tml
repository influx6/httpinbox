{{ define "content" }}
<section class="inbox">

  <div class="title">
    <label>{{.Inbox}}</label>
  </div>
  <div class="items">
   <ul>
    {{ $inbox := .Inbox }}
    {{ range $k, $v := .Items }}
      <li><a href="/inbox/{{$inbox}}/{{$v.Name}}">{{$v.Name}}</a></li>
    {{end}}
    </ul>
  </div>
</section>
{{ end }}
